{
  "name": "donald-trash",
  "version": "0.0.1",
  "description": "Example about using React DnD for greater good",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production node bin/compile",
    "build": "npm run clean && npm run build:webpack",
    "start": "NODE_ENV=development node devServer.js",
    "clean-source": "rimraf bin README.md .babelrc devServer.js src screenshot.png webpack LICENSE .eslintrc package.json karma.conf.js tests.webpack.js",
    "deploy": "gh-pages-deploy",
    "lint": "eslint src",
    "test": "karma start",
    "test-watch": "karma start --watch",
    "test-multi": "karma start --single-run=false"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "build"
    ],
    "post": [
      "clean-source"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andresgutgon/donald-trash.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "dnd",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Andres Gutierrez <andresgutgon@gmail.com> (http://github.com/andresgutgon)",
  "homepage": "https://github.com/andresgutgon/test-newr",
  "devDependencies": {
    "autoprefixer-core": "^5.1.11",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "connect-history-api-fallback": "^1.1.0",
    "css-loader": "^0.15.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "font-awesome-webpack": "0.0.4",
    "gh-pages-deploy": "^0.4.0",
    "html-loader": "0.4.0",
    "html-webpack-plugin": "2.7.2",
    "postcss-cssnext": "2.4.0",
    "postcss-import": "7.1.3",
    "postcss-loader": "0.8.0",
    "postcss-nested": "1.0.0",
    "react-transform-hmr": "^1.0.1",
    "rimraf": "^2.4.3",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "normalize.css": "^3.0.3",
    "react": "^0.14.7",
    "react-dnd": "^2.1.3",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^0.14.3"
  }
}
